{
  "name": "@tui-testing/core",
  "version": "0.1.0",
  "description": "Generic TUI testing framework - tmux controller and test reporter",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./tmux-controller": {
      "import": "./dist/tmux-controller.js",
      "types": "./dist/tmux-controller.d.ts"
    },
    "./reporter": {
      "import": "./dist/reporter.js",
      "types": "./dist/reporter.d.ts"
    },
    "./state-observer": {
      "import": "./dist/state-observer.js",
      "types": "./dist/state-observer.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "node --test dist/**/*.test.js"
  },
  "keywords": [
    "tui",
    "tmux",
    "testing",
    "automation",
    "terminal",
    "reporter"
  ],
  "author": "Canvas Testing Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/tui-testing"
  },
  "bugs": {
    "url": "https://github.com/your-org/tui-testing/issues"
  },
  "dependencies": {
    "typescript": "^5.9.3"
  }
}
