{
  "name": "@tui-testing/framework",
  "version": "1.0.0",
  "description": "Production-ready TUI testing framework - test any terminal application",
  "type": "module",
  "private": false,
  "scripts": {
    "build": "cd core && bun run build",
    "test": "cd core && bun test",
    "clean": "find . -type d -name dist -o -name node_modules | xargs rm -rf",
    "format": "prettier --write '**/*.ts'",
    "lint": "cd core && tsc --noEmit"
  },
  "keywords": [
    "tui",
    "testing",
    "tmux",
    "terminal",
    "automation",
    "canvas",
    "cli",
    "framework"
  ],
  "author": "Canvas Testing Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/tui-testing"
  },
  "bugs": {
    "url": "https://github.com/your-org/tui-testing/issues"
  },
  "homepage": "https://github.com/your-org/tui-testing#readme",
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
